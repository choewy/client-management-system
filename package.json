{
  "name": "client-management-system",
  "version": "1.0.0",
  "description": "고객 관리 시스템",
  "scripts": {
    "setting": "cd client & npm install & cd ../server & npm install & cd ../",
    "client": "cd client & npm run start",
    "server": "cd server & npm run start:dev",
    "start:dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choewy/client-management-system.git"
  },
  "author": "choewy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/choewy/client-management-system/issues"
  },
  "homepage": "https://github.com/choewy/client-management-system#readme",
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}